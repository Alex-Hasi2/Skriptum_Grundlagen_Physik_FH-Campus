"workbench.colorTheme": "Overleaf Theme",
"editor.tokenColorCustomizations": {
    "[Overleaf Theme]": {
      "comments": "#40a6c5", 
      "keywords": "#00a128",
      "functions": "#008f24",
      "variables": "#bb76e3",
      "types": "#353535",
      "numbers": "#63546a", 
      "textMateRules": [
        {
          "scope": 
            ["constant.other.general.math.tex", "constant.character.math.tex", "constant.other.math.tex",
            "entity.name.section.latex", "punctuation.definition.constant.math.tex"], 
          "settings": {"foreground": "#000000"}
        },
        {
          "scope": "markup.bold.textbf.latex",
          "settings": {"foreground": "#000000", "fontStyle": "bold"}
        },
        {
          "scope": "constant.other.reference.label.latex",
          "settings": {"foreground": "#62009b"}
        },
        {
          "scope": "support.class.math.block.tex",
          "settings": {"foreground": "#000000"}
        },
        {
          "scope": ["punctuation.definition.string.begin.tex", "punctuation.definition.string.end.tex"],
          "settings": {"foreground": "#e37351"}
        },
        {
          "scope": "variable.parameter.function.latex",
          "settings": {"foreground": "#8C8DC6"}
        }
      ]
    }
  },
  "workbench.colorCustomizations": {
    "[Overleaf Theme]": {
      "input.background": "#a4a4a4",
      "input.foreground": "#000000",
      "input.placeholderForeground": "#666666",
      "input.border": "#cccccc",
      "titleBar.activeForeground": "#a3cf9e",
      "titleBar.inactiveForeground": "#333333",
      "searchEditor.textInputBorder": "#cccccc",
      "sideBar.foreground": "#e7e7e7",
      "searchEditor.textInputBorder": "#cccccc",
      "panel.foreground": "#000000",
      "panelTitleBadge.background": "#de8989",
      "sideBarTitle.background": "#738399",
      "sideBarTitle.foreground":"#ffffff"
      }
  },
  "editor.minimap.showSlider": "always",
  "editor.minimap.enabled": false,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
